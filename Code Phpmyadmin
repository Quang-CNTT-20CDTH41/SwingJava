create table student(
    id int primary key auto_increment,           // tự tạo id
    fullname varchar(50) not null,          //tạo cột fullname
    gender varchar(15),                     //tạo cột gender
    age int,                                //tạo cột số age
    email varchar(150),                     // ký tự mail
    phone_number varchar(15)                 //ký tự sdt
)
